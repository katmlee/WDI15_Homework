<%= form_for @book do |f| %>
<fieldset>
  <%= f.label :title %>
  <%= f.text_field :title %>
</fieldset>
<fieldset>
  <%= f.label :image %>
  <%= f.url_field :image %>
</fieldset>
<fieldset>
  <%= f.label :year %>
  <%= f.text_field :year %>
</fieldset>
<fieldset>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</fieldset>
<fieldset>
  <%= f.label :synopsis %>
  <%= f.text_field :synopsis %>
</fieldset>
<%= f.submit %>
<% end %>
